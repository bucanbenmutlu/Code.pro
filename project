n = int(input("Enter a number: "))
for i in range(1, 11):
    print(f"{n} x {i} = {n * i}")

likes = [10, 3, 25, 7]

total = 0
for like in likes:
    total += like

print("Total likes:", total)

distance = float(input("Enter distance in km: "))

if distance < 2:
    print("Estimated fare: $5")
elif distance < 5:
    print("Estimated fare: $10")
else:
    print("Estimated fare: $15")
